{
  "name": "agentic-wallet-solana-devnet",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.6.0",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "lint": "pnpm -r lint",
    "test": "pnpm --filter backend test",
    "anchor:build": "anchor build --provider.cluster localnet --provider.wallet ~/.config/solana/id.json",
    "anchor:test": "anchor test --skip-build",
    "anchor:deploy:devnet": "anchor deploy --provider.cluster devnet",
    "demo:devnet": "pnpm --filter backend demo:devnet"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "eslint": "^9.21.0",
    "prettier": "^3.5.2",
    "typescript": "^5.8.2"
  }
}
